{% extends 'ThemeBundle:Default:layout_base.html.twig' %}

{% block content_title %}
   Resumen
{% endblock %}

{% block content -%}
<div class="row">
    <div class="col-md-6">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">Medida</h3>
            </div>
            <div class="box-body">
                <table class="record_properties">
                    <tbody>
                        <tr>
                            <th>Id</th>
                            <td>{{ entity.id }}</td>
                        </tr>
                        <tr>
                            <th>Ultimoregistro</th>
                            <td>{{ entity.ultimoRegistro }}</td>
                        </tr>
                        <tr>
                            <th>Variacion</th>
                            <td>{{ entity.variacion }}</td>
                        </tr>
                        <tr>
                            <th>Periodo</th>
                            <td>{{ entity.periodo }}</td>
                        </tr>
                        <tr>
                            <th>Fechahora</th>
                            <td>{{ entity.fechaHora|date('Y-m-d H:i:s') }}</td>
                        </tr>
                        <tr>
                            <th>Alerta</th>
                            <td>{{ entity.alerta }}</td>
                        </tr>
                        <tr>
                            <th>Evacuacion</th>
                            <td>{{ entity.evacuacion }}</td>
                        </tr>
                        <tr>
                            <th>Creado</th>
                            <td>{{ entity.creado|date('Y-m-d H:i:s') }}</td>
                        </tr>
                        <tr>
                            <th>Actualizado</th>
                            <td>{{ entity.actualizado|date('Y-m-d H:i:s') }}</td>
                        </tr>
                        <tr>
                            <th>Fuentedatos</th>
                            <td>{{ entity.fuenteDatos }}</td>
                        </tr>
                    </tbody>
                </table>
            </div>


        <div class="box-footer">
<a class="btn btn-default" href="{{ path('medidas') }}">
    Volver
</a>


        <a class="btn btn-primary" href="{{ path('medidas_edit', { 'id': entity.id }) }}">
            Editar
        </a>

</div>
</div>
</div>
</div>
    {{ form(delete_form) }}


{% endblock %}
